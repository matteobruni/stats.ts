{
  "name": "stats.ts",
  "version": "2.0.0",
  "description": "JavaScript Performance Monitor",
  "type": "module",
  "browser": "dist/cjs/index.js",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "jsdelivr": "dist/stats.min.js",
  "unpkg": "dist/stats.min.js",
  "repository": "matteobruni/stats.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prettify:src": "prettier --write ./src/",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "clear:dist": "rimraf ./dist",
    "build": "pnpm run clear:dist && pnpm run prettify:src && pnpm run build:ts:check && pnpm run lint && pnpm run build:ts && webpack --mode production",
    "build:ts:check": "tsc --noEmit",
    "build:ts": "pnpm run build:ts:browser && pnpm run build:ts:cjs",
    "build:ts:browser": "tsc",
    "build:ts:cjs": "tsc -b tsconfig.cjs.json",
    "prepack": "pnpm run build"
  },
  "keywords": [
    "performance",
    "fps",
    "stats"
  ],
  "author": "matteobruni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matteobruni/stats.ts/issues"
  },
  "homepage": "https://github.com/matteobruni/stats.ts",
  "devDependencies": {
    "@eslint/js": "^9.39.3",
    "@babel/core": "^7.29.0",
    "@stylistic/eslint-plugin": "^5.9.0",
    "babel-loader": "^10.0.0",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^10.1.8",
    "fs-extra": "^11.3.3",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.3",
    "terser-webpack-plugin": "^5.3.16",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "webpack": "^5.105.2",
    "webpack-bundle-analyzer": "^5.2.0",
    "webpack-cli": "^6.0.1"
  },
  "packageManager": "pnpm@10.30.0"
}
